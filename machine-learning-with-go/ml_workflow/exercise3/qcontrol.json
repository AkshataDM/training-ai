{
  "pipeline": {
    "name": "qcontrol"
  },
  "transform": {
    "image": "gopherdata/gc2018:qc",
    "cmd": [ 
      "/qcontrol", 
      "-inModelFile=/pfs/model/model.json", 
      "-inTestDir=/pfs/qc_pre_process/",
      "-threshold=60.0", 
      "-outDir=/pfs/out"       
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "cross": [
      {
        "atom": {
          "repo": "qc_pre_process",
          "glob": "/"
	}
      },
      {
	"atom": {
          "repo": "model",
          "glob": "/"
	}
      }
    ]
  }
}
